        enum Tokens{TYPE_SPECIFIER=0x100,
                    KEYWORD,
                    IDENTIFIER,
		            INT_CONST,
                    FLOAT_CONST,
                    STRING_LITERAL,
                    EQ_OP,
                    NE_OP,
                    LE_OP,
                    GE_OP,
                    AND_OP,
                    OR_OP,
                    INC_OP,
                    OTHERS};
