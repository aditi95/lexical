        enum Tokens{VOID=0x100,
                    INT,
                    FLOAT,
                    IF,
                    ELSE,
                    FOR,
                    WHILE,
                    RETURN,
                    IDENTIFIER,
		          INT_CONST,
                    FLOAT_CONST,
                    STRING_LITERAL,
                    EQ_OP,
                    NE_OP,
                    LE_OP,
                    GE_OP,
                    AND_OP,
                    OR_OP,
                    INC_OP,
                    OTHERS};
